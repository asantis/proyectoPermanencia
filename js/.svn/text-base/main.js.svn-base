$(buscar_dato());
function buscar_dato(consulta){
	$.ajax({
		url: '../Buscadores/buscar.php',
		type: 'POST',
		dataType:'html',
		data: {consulta: consulta},
	})
	.done(function(respuesta){
		$("#datos").html(respuesta);
	})
	.fail(function(){
			console.log("error");
		})
}


$(document).on('click', '#patente', function(){
	var valor = $(this).val();
	if(valor != ""){
			buscar_dato(valor);
		}else{
			buscar_dato();
		}
});
